{
  "buildCommand": "npx expo export",
  "outputDirectory": "dist",
  "framework": null,
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/admin/lead/:id",
      "destination": "/admin/lead/[id].html"
    },
    {
      "source": "/blog/:id",
      "destination": "/blog/[id].html"
    }
  ],
  "headers": [
    {
      "source": "/_expo/static/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
