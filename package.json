    "expo-web-browser": "~15.0.10",
    "libphonenumber-js": "^1.12.34",
    "lucide-react-native": "^0.562.0",
    "react": "19.1.0",
    "react-dom": "^19.1.0",
    "react-ga4": "^2.1.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
