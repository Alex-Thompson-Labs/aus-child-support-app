    "eslint": "^9.39.2",
    "eslint-config-expo": "~10.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react": "^7.37.5",
    "expo-atlas": "^0.4.0",
    "fast-check": "^4.5.3",
    "jest": "~29.7.0",
    "jest-expo": "^54.0.16",
    "lighthouse": "^13.0.1",
    "react-test-renderer": "19.1.0",
    "serve": "^14.2.5",
